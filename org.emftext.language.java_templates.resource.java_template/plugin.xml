<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java_templates.resource.java_template.Java_templateResourceFactory" type="java_template">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.syntax">
		<metaInformationProvider class="org.emftext.language.java_templates.resource.java_template.Java_templateMetaInformation" id="org.emftext.language.java_templates.resource.java_template.Java_templateMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="java_template">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java_templates.resource.java_template.Java_templateNewFileWizard" id="org.emftext.language.java_templates.resource.java_template.Java_templateNewFileWizard" name="EMFText .java_template file">
		</wizard>
	</extension>

   <extension
         point="org.emftext.runtime.default_load_options">
      <provider
            class="org.emftext.language.java.resource.java.analysis.helper.JavaPostProcessor"
            csName="java_template"
            id="org.emftext.language.java.expression_simplifier_option_provider">
      </provider>
   </extension>
   	
</plugin>
