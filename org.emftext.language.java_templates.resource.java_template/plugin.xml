<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.java_templates.resource.java_template.mopp.Java_templateMetaInformation" id="org.emftext.language.java_templates.resource.java_template.mopp.Java_templateMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.java_templates.resource.java_template.ui.Java_templateEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="java_template" icon="icons/editor_icon.gif" id="org.emftext.language.java_templates.resource.java_template.ui.Java_templateEditor" name="EMFText java_template Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.java_templates.resource.java_template.ui.Java_templatePreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Java_template Text Editor" id="org.emftext.language.java_templates.resource.java_template.ui.Java_templatePreferencePage" class="org.emftext.language.java_templates.resource.java_template.ui.Java_templatePreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.java_templates.resource.java_template.ui.Java_templateSyntaxColoringPreferencePage" class="org.emftext.language.java_templates.resource.java_template.ui.Java_templateSyntaxColoringPreferencePage" category="org.emftext.language.java_templates.resource.java_template.ui.Java_templatePreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.java_templates.resource.java_template.ui.Java_templateBracketPreferencePage" class="org.emftext.language.java_templates.resource.java_template.ui.Java_templateBracketPreferencePage" category="org.emftext.language.java_templates.resource.java_template.ui.Java_templatePreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.java_templates.resource.java_template.ui.Java_templateOccurrencePreferencePage" class="org.emftext.language.java_templates.resource.java_template.ui.Java_templateOccurrencePreferencePage" category="org.emftext.language.java_templates.resource.java_template.ui.Java_templatePreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java_templates.resource.java_template.ui.Java_templateNewFileWizard" id="org.emftext.language.java_templates.resource.java_template.ui.Java_templateNewFileWizard" name="EMFText .java_template file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.java_templates.resource.java_template.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.java_templates.resource.java_template.nature" name="java_template nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.java_templates.resource.java_template.mopp.Java_templateNature" />
		</runtime>
		<builder id="org.emftext.language.java_templates.resource.java_template.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.java_templates.resource.java_template.builder" name="java_template Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.java_templates.resource.java_template.mopp.Java_templateBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.java_templates.resource.java_template.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java_templates.resource.java_template.mopp.Java_templateResourceFactoryDelegator" type="java_template">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.java_templates.resource.java_template.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
