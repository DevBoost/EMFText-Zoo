<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.uml_class.resource.uml_class.Uml_classResourceFactory" type="uml_class">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.syntax">
		<metaInformationProvider class="org.emftext.language.uml_class.resource.uml_class.Uml_classMetaInformation" id="org.emftext.language.uml_class.resource.uml_class.Uml_classMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="uml_class">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.uml_class.resource.uml_class.Uml_classNewFileWizard" id="org.emftext.language.uml_class.resource.uml_class.Uml_classNewFileWizard" name="EMFText .uml_class file">
		</wizard>
	</extension>
	
</plugin>
