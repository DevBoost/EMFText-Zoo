<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.uml_class.resource.uml_class.mopp.Uml_classMetaInformation" id="org.emftext.language.uml_class.resource.uml_class.mopp.Uml_classMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="uml_class" icon="icons/editor_icon.gif" id="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classEditor" name="EMFText uml_class Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Uml_class Text Editor" id="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classPreferencePage" class="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classPreferencePage">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classSyntaxColoringPreferencePage" class="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classSyntaxColoringPreferencePage" category="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classBracketPreferencePage" class="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classBracketPreferencePage" category="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classOccurrencePreferencePage" class="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classOccurrencePreferencePage" category="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classNewFileWizard" id="org.emftext.language.uml_class.resource.uml_class.ui.Uml_classNewFileWizard" name="EMFText .uml_class file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.uml_class.resource.uml_class.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.language.uml_class.resource.uml_class.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.uml_class.resource.uml_class.mopp.Uml_classResourceFactoryDelegator" type="uml_class">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.uml_class.resource.uml_class.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
