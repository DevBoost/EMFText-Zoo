<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.valueflow.resource.valueflow.TextValueflowResourceFactory" type="text.valueflow">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.syntax">
		<metaInformationProvider class="org.emftext.language.valueflow.resource.valueflow.TextValueflowMetaInformation" id="org.emftext.language.valueflow.resource.valueflow.TextValueflowMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="text.valueflow">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.valueflow.resource.valueflow.TextValueflowNewFileWizard" id="org.emftext.language.valueflow.resource.valueflow.TextValueflowNewFileWizard" name="EMFText .text.valueflow file">
		</wizard>
	</extension>
	
</plugin>
