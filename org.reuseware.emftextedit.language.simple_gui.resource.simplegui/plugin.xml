<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.reuseware.emftextedit.language.simple_gui.resource.simplegui.SimpleguiResourceFactoryImpl"
            type="simplegui">
      </parser>
   </extension>

   <extension
         point="org.emftext.runtime.concretesyntax">
      <concretesyntax
            uri="http://www.reuseware.org/simple_gui"
            csName="simplegui"
            csDefinition="org.reuseware.emftextedit.language.simple_gui/metamodel/simple_gui.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.emftext.filetype"
            file-extensions="simplegui">
      </file-association>
   </extension>

</plugin>
