<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.eclipse.bpel.model.resource.bpws.BpwsResourceFactoryImpl"
            type="bpws">
      </parser>
   </extension>

   <extension
         point="org.reuseware.emftextedit.runtime.concretesyntax">
      <concretesyntax
            uri="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
            csName="bpws"
            csDefinition="org.reuseware.emftextedit.language.bpel/metamodel/bpel.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.reuseware.emftextedit.filetype"
            file-extensions="bpws">
      </file-association>
   </extension>

</plugin>
