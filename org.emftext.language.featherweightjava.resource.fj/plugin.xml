<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.featherweightjava.resource.fj.mopp.FjMetaInformation" id="org.emftext.language.featherweightjava.resource.fj.mopp.FjMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.featherweightjava.resource.fj.ui.FjEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="fj" icon="icons/editor_icon.gif" id="org.emftext.language.featherweightjava.resource.fj.ui.FjEditor" name="EMFText fj Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.featherweightjava.resource.fj.ui.FjPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Fj Text Editor" id="org.emftext.language.featherweightjava.resource.fj.ui.FjPreferencePage" class="org.emftext.language.featherweightjava.resource.fj.ui.FjPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.featherweightjava.resource.fj.ui.FjSyntaxColoringPreferencePage" class="org.emftext.language.featherweightjava.resource.fj.ui.FjSyntaxColoringPreferencePage" category="org.emftext.language.featherweightjava.resource.fj.ui.FjPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.featherweightjava.resource.fj.ui.FjBracketPreferencePage" class="org.emftext.language.featherweightjava.resource.fj.ui.FjBracketPreferencePage" category="org.emftext.language.featherweightjava.resource.fj.ui.FjPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.featherweightjava.resource.fj.ui.FjOccurrencePreferencePage" class="org.emftext.language.featherweightjava.resource.fj.ui.FjOccurrencePreferencePage" category="org.emftext.language.featherweightjava.resource.fj.ui.FjPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.featherweightjava.resource.fj.ui.FjNewFileWizard" id="org.emftext.language.featherweightjava.resource.fj.ui.FjNewFileWizard" name="EMFText .fj file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.featherweightjava.resource.fj.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.featherweightjava.resource.fj.nature" name="fj nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.featherweightjava.resource.fj.mopp.FjNature" />
		</runtime>
		<builder id="org.emftext.language.featherweightjava.resource.fj.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.featherweightjava.resource.fj.builder" name="fj Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.featherweightjava.resource.fj.mopp.FjBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.featherweightjava.resource.fj.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.featherweightjava.resource.fj.mopp.FjResourceFactoryDelegator" type="fj">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.featherweightjava.resource.fj.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
