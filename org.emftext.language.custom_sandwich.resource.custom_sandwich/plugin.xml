<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.custom_sandwich.resource.custom_sandwich.mopp.Custom_sandwichResourceFactory" type="custom_sandwich">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.custom_sandwich.resource.custom_sandwich.mopp.Custom_sandwichTextResourcePluginMetaInformation" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.mopp.Custom_sandwichTextResourcePluginMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="org.emftext.filetype.custom_sandwich" name="EMFText custom_sandwich Files">
		</content-type>
		<file-association content-type="org.emftext.filetype.custom_sandwich" file-extensions="custom_sandwich">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" icon="icons/editor_icon.gif" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichEditor" name="EMFText custom_sandwich Editor">
			<contentTypeBinding contentTypeId="org.eclipse.core.runtime.text">
			</contentTypeBinding>
			<contentTypeBinding contentTypeId="org.emftext.filetype.custom_sandwich" >
			</contentTypeBinding>
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichNewFileWizard" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichNewFileWizard" name="EMFText .custom_sandwich file">
		</wizard>
	</extension>
	
	<extension-point id="org.emftext.language.custom_sandwich.resource.custom_sandwich.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
</plugin>
