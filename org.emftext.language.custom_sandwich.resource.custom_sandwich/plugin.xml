<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.custom_sandwich.resource.custom_sandwich.mopp.Custom_sandwichMetaInformation" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.mopp.Custom_sandwichMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="custom_sandwich" icon="icons/editor_icon.gif" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichEditor" name="EMFText custom_sandwich Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Custom_sandwich Text Editor" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichPreferencePage" class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichSyntaxColoringPreferencePage" class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichSyntaxColoringPreferencePage" category="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichBracketPreferencePage" class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichBracketPreferencePage" category="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichOccurrencePreferencePage" class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichOccurrencePreferencePage" category="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichNewFileWizard" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.ui.Custom_sandwichNewFileWizard" name="EMFText .custom_sandwich file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.custom_sandwich.resource.custom_sandwich.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.custom_sandwich.resource.custom_sandwich.nature" name="custom_sandwich nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.custom_sandwich.resource.custom_sandwich.mopp.Custom_sandwichNature" />
		</runtime>
		<builder id="org.emftext.language.custom_sandwich.resource.custom_sandwich.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.builder" name="custom_sandwich Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.custom_sandwich.resource.custom_sandwich.mopp.Custom_sandwichBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.custom_sandwich.resource.custom_sandwich.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.custom_sandwich.resource.custom_sandwich.mopp.Custom_sandwichResourceFactoryDelegator" type="custom_sandwich">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.custom_sandwich.resource.custom_sandwich.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
