<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.custom_sandwich.resource.custom_sandwich.Custom_sandwichResourceFactory" type="custom_sandwich">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.concretesyntax">
		<concretesyntax uri="http://www.emftext.org/language/custom_sandwich" csName="custom_sandwich" csDefinition="org.emftext.language.custom_sandwich/metamodel/custom_sandwich.cs">
		</concretesyntax>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="custom_sandwich">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.custom_sandwich.resource.custom_sandwich.Custom_sandwichNewFileWizard" id="org.emftext.language.custom_sandwich.resource.custom_sandwich.Custom_sandwichNewFileWizard" name="EMFText .custom_sandwich file">
		</wizard>
	</extension>
	
</plugin>
