<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.reuseware.emftextedit.language.simple_math.resource.sm.SmResourceFactoryImpl"
            type="sm">
      </parser>
   </extension>

   <extension
         point="org.reuseware.emftextedit.runtime.concretesyntax">
      <concretesyntax
            uri="http://www.reuseware.org/simple_math"
            csName="sm"
            csDefinition="org.reuseware.emftextedit.language.simple_math/metamodel/simple_math.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.reuseware.emftextedit.filetype"
            file-extensions="sm">
      </file-association>
   </extension>

</plugin>
