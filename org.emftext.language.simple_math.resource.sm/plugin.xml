<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.simple_math.resource.sm.mopp.SmResourceFactory" type="sm">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.simple_math.resource.sm.mopp.SmMetaInformation" id="org.emftext.language.simple_math.resource.sm.mopp.SmMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="org.emftext.filetype.sm" name="EMFText sm Files">
		</content-type>
		<file-association content-type="org.emftext.filetype.sm" file-extensions="sm">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.simple_math.resource.sm.ui.SmEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" icon="icons/editor_icon.gif" id="org.emftext.language.simple_math.resource.sm.ui.SmEditor" name="EMFText sm Editor">
			<contentTypeBinding contentTypeId="org.eclipse.core.runtime.text">
			</contentTypeBinding>
			<contentTypeBinding contentTypeId="org.emftext.filetype.sm" >
			</contentTypeBinding>
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.simple_math.resource.sm.ui.SmPreferenceInitializer">
		</initializer>
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.simple_math.resource.sm.ui.SmNewFileWizard" id="org.emftext.language.simple_math.resource.sm.ui.SmNewFileWizard" name="EMFText .sm file">
		</wizard>
	</extension>
	
</plugin>
