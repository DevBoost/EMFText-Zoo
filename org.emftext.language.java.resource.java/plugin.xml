<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.java.resource.java.mopp.JavaMetaInformation" id="org.emftext.language.java.resource.java.mopp.JavaMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.java.resource.java.ui.JavaEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="java" icon="icons/editor_icon.gif" id="org.emftext.language.java.resource.java.ui.JavaEditor" name="EMFText java Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.java.resource.java.ui.JavaPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Java Text Editor" id="org.emftext.language.java.resource.java.ui.JavaPreferencePage" class="org.emftext.language.java.resource.java.ui.JavaPreferencePage">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.java.resource.java.ui.JavaSyntaxColoringPreferencePage" class="org.emftext.language.java.resource.java.ui.JavaSyntaxColoringPreferencePage" category="org.emftext.language.java.resource.java.ui.JavaPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.java.resource.java.ui.JavaBracketPreferencePage" class="org.emftext.language.java.resource.java.ui.JavaBracketPreferencePage" category="org.emftext.language.java.resource.java.ui.JavaPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.java.resource.java.ui.JavaOccurrencePreferencePage" class="org.emftext.language.java.resource.java.ui.JavaOccurrencePreferencePage" category="org.emftext.language.java.resource.java.ui.JavaPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java.resource.java.ui.JavaNewFileWizard" id="org.emftext.language.java.resource.java.ui.JavaNewFileWizard" name="EMFText .java file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.java.resource.java.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.language.java.resource.java.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java.resource.java.mopp.JavaResourceFactoryDelegator" type="java">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.java.resource.java.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
