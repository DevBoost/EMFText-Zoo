  <namespaces>java</namespaces>
  <namespaces>applet</namespaces>
  <classifiers xsi:type="classifiers:Interface" name="AudioClip">
    <members xsi:type="members:InterfaceMethod" name="play">
      <typeReference xsi:type="types:Void">
        <comments>/**&#xA;     * Starts playing this audio clip. Each time this method is called,&#xA;     * the clip is restarted from the beginning.&#xA;     */</comments>
      </typeReference>
    </members>
    <members xsi:type="members:InterfaceMethod" name="loop">
      <typeReference xsi:type="types:Void">
        <comments>/**&#xA;     * Starts playing this audio clip in a loop.&#xA;     */</comments>
      </typeReference>
    </members>
    <members xsi:type="members:InterfaceMethod" name="stop">
      <typeReference xsi:type="types:Void">
        <comments>/**&#xA;     * Stops playing this audio clip.&#xA;     */</comments>
      </typeReference>
    </members>
    <annotationsAndModifiers xsi:type="modifiers:Public">
      <comments>/**&#xA; * The &lt;code>AudioClip&lt;/code> interface is a simple abstraction for&#xA; * playing a sound clip. Multiple &lt;code>AudioClip&lt;/code> items can be&#xA; * playing at the same time, and the resulting sound is mixed&#xA; * together to produce a composite.&#xA; *&#xA; * @author &#x9;Arthur van Hoff&#xA; * @version     1.18, 12/19/03&#xA; * @since       JDK1.0&#xA; */</comments>
    </annotationsAndModifiers>
  </classifiers>
