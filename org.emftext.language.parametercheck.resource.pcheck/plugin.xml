<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.parametercheck.resource.pcheck.mopp.PcheckMetaInformation" id="org.emftext.language.parametercheck.resource.pcheck.mopp.PcheckMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="pcheck" icon="icons/editor_icon.gif" id="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckEditor" name="EMFText pcheck Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Pcheck Text Editor" id="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckPreferencePage" class="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckPreferencePage">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckSyntaxColoringPreferencePage" class="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckSyntaxColoringPreferencePage" category="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckBracketPreferencePage" class="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckBracketPreferencePage" category="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckOccurrencePreferencePage" class="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckOccurrencePreferencePage" category="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckNewFileWizard" id="org.emftext.language.parametercheck.resource.pcheck.ui.PcheckNewFileWizard" name="EMFText .pcheck file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.parametercheck.resource.pcheck.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.language.parametercheck.resource.pcheck.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.parametercheck.resource.pcheck.mopp.PcheckResourceFactoryDelegator" type="pcheck">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.parametercheck.resource.pcheck.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
