<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.emftext.language.c_sharp.resource.cs.CsResourceFactoryImpl"
            type="cs">
      </parser>
   </extension>

   <extension
         point="org.emftext.runtime.concretesyntax">
      <concretesyntax
            uri="http://www.emftext.org/c_sharp"
            csName="cs"
            csDefinition="org.emftext.language.c_sharp/metamodel/c_sharp.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.emftext.filetype"
            file-extensions="cs">
      </file-association>
   </extension>

</plugin>
