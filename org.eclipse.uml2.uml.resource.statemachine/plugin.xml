<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.eclipse.uml2.uml.resource.statemachine.StatemachineResourceFactory" type="statemachine">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.concretesyntax">
		<concretesyntax uri="http://www.eclipse.org/uml2/2.1.0/UML" csName="statemachine" csDefinition="org.emftext.language.statemachine/metamodel/statemachine.cs">
		</concretesyntax>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="statemachine">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="\icons\default_new_icon.gif" class="org.eclipse.uml2.uml.resource.statemachine.StatemachineNewFileAction" id="org.eclipse.uml2.uml.resource.statemachine.StatemachineNewFileAction" name="EMFText .statemachine file">
		</wizard>
	</extension>
	
</plugin>
