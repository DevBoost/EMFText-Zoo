<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.eclipse.uml2.uml.resource.statemachine.StatemachineResourceFactoryImpl"
            type="statemachine">
      </parser>
   </extension>

   <extension
         point="org.reuseware.emftextedit.runtime.concretesyntax">
      <concretesyntax
            uri="http://www.eclipse.org/uml2/2.1.0/UML"
            csName="statemachine"
            csDefinition="org.reuseware.emftextedit.language.statemachine/metamodel/statemachine.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.reuseware.emftextedit.filetype"
            file-extensions="statemachine">
      </file-association>
   </extension>

</plugin>
