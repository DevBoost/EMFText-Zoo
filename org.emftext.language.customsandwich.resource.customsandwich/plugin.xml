<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichMetaInformation" id="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="customsandwich" icon="icons/editor_icon.gif" id="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichEditor" name="EMFText customsandwich Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Customsandwich Text Editor" id="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichPreferencePage" class="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichSyntaxColoringPreferencePage" class="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichSyntaxColoringPreferencePage" category="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichBracketPreferencePage" class="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichBracketPreferencePage" category="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichOccurrencePreferencePage" class="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichOccurrencePreferencePage" category="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichNewFileWizard" id="org.emftext.language.customsandwich.resource.customsandwich.ui.CustomsandwichNewFileWizard" name="EMFText .customsandwich file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.customsandwich.resource.customsandwich.nature" name="customsandwich nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichNature" />
		</runtime>
		<builder id="org.emftext.language.customsandwich.resource.customsandwich.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.customsandwich.resource.customsandwich.builder" name="customsandwich Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.customsandwich.resource.customsandwich.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.customsandwich.resource.customsandwich.mopp.CustomsandwichResourceFactoryDelegator" type="customsandwich">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.customsandwich.resource.customsandwich.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
