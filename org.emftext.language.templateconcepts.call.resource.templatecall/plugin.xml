<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.templateconcepts.call.resource.templatecall.mopp.TemplatecallMetaInformation" id="org.emftext.language.templateconcepts.call.resource.templatecall.mopp.TemplatecallMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="templatecall" icon="icons/editor_icon.gif" id="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallEditor" name="EMFText templatecall Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Templatecall Text Editor" id="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallPreferencePage" class="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallSyntaxColoringPreferencePage" class="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallSyntaxColoringPreferencePage" category="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallBracketPreferencePage" class="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallBracketPreferencePage" category="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallOccurrencePreferencePage" class="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallOccurrencePreferencePage" category="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallNewFileWizard" id="org.emftext.language.templateconcepts.call.resource.templatecall.ui.TemplatecallNewFileWizard" name="EMFText .templatecall file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.templateconcepts.call.resource.templatecall.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.templateconcepts.call.resource.templatecall.nature" name="templatecall nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.templateconcepts.call.resource.templatecall.mopp.TemplatecallNature" />
		</runtime>
		<builder id="org.emftext.language.templateconcepts.call.resource.templatecall.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.templateconcepts.call.resource.templatecall.builder" name="templatecall Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.templateconcepts.call.resource.templatecall.mopp.TemplatecallBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.templateconcepts.call.resource.templatecall.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.templateconcepts.call.resource.templatecall.mopp.TemplatecallResourceFactoryDelegator" type="templatecall">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.templateconcepts.call.resource.templatecall.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
