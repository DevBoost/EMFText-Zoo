<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.sparql.resource.sparql.mopp.SparqlMetaInformation" id="org.emftext.language.sparql.resource.sparql.mopp.SparqlMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.sparql.resource.sparql.ui.SparqlEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="sparql" icon="icons/editor_icon.gif" id="org.emftext.language.sparql.resource.sparql.ui.SparqlEditor" name="EMFText sparql Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.sparql.resource.sparql.ui.SparqlPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Sparql Text Editor" id="org.emftext.language.sparql.resource.sparql.ui.SparqlPreferencePage" class="org.emftext.language.sparql.resource.sparql.ui.SparqlPreferencePage">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.sparql.resource.sparql.ui.SparqlSyntaxColoringPreferencePage" class="org.emftext.language.sparql.resource.sparql.ui.SparqlSyntaxColoringPreferencePage" category="org.emftext.language.sparql.resource.sparql.ui.SparqlPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.sparql.resource.sparql.ui.SparqlBracketPreferencePage" class="org.emftext.language.sparql.resource.sparql.ui.SparqlBracketPreferencePage" category="org.emftext.language.sparql.resource.sparql.ui.SparqlPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.sparql.resource.sparql.ui.SparqlOccurrencePreferencePage" class="org.emftext.language.sparql.resource.sparql.ui.SparqlOccurrencePreferencePage" category="org.emftext.language.sparql.resource.sparql.ui.SparqlPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.sparql.resource.sparql.ui.SparqlNewFileWizard" id="org.emftext.language.sparql.resource.sparql.ui.SparqlNewFileWizard" name="EMFText .sparql file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.sparql.resource.sparql.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.language.sparql.resource.sparql.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.sparql.resource.sparql.mopp.SparqlResourceFactoryDelegator" type="sparql">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.sparql.resource.sparql.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
