<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.sparql.resource.sparql.mopp.SparqlResourceFactory" type="sparql">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.sparql.resource.sparql.mopp.SparqlMetaInformation" id="org.emftext.language.sparql.resource.sparql.mopp.SparqlMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="org.emftext.filetype.sparql" name="EMFText sparql Files">
		</content-type>
		<file-association content-type="org.emftext.filetype.sparql" file-extensions="sparql">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.sparql.resource.sparql.ui.SparqlEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" icon="icons/editor_icon.gif" id="org.emftext.language.sparql.resource.sparql.ui.SparqlEditor" name="EMFText sparql Editor">
			<contentTypeBinding contentTypeId="org.eclipse.core.runtime.text">
			</contentTypeBinding>
			<contentTypeBinding contentTypeId="org.emftext.filetype.sparql" >
			</contentTypeBinding>
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.sparql.resource.sparql.ui.SparqlPreferenceInitializer">
		</initializer>
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.sparql.resource.sparql.ui.SparqlNewFileWizard" id="org.emftext.language.sparql.resource.sparql.ui.SparqlNewFileWizard" name="EMFText .sparql file">
		</wizard>
	</extension>
	
</plugin>
