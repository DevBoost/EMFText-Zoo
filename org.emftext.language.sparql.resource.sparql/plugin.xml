<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.sparql.resource.sparql.SparqlResourceFactory" type="sparql">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.syntax">
		<metaInformationProvider class="org.emftext.language.sparql.resource.sparql.SparqlMetaInformation" id="org.emftext.language.sparql.resource.sparql.SparqlMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="sparql">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.sparql.resource.sparql.SparqlNewFileWizard" id="org.emftext.language.sparql.resource.sparql.SparqlNewFileWizard" name="EMFText .sparql file">
		</wizard>
	</extension>
	
</plugin>
