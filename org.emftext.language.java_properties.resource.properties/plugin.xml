<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.java_properties.resource.properties.mopp.PropertiesMetaInformation" id="org.emftext.language.java_properties.resource.properties.mopp.PropertiesMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.java_properties.resource.properties.ui.PropertiesEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="properties" icon="icons/editor_icon.gif" id="org.emftext.language.java_properties.resource.properties.ui.PropertiesEditor" name="EMFText properties Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.java_properties.resource.properties.ui.PropertiesPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Properties Text Editor" id="org.emftext.language.java_properties.resource.properties.ui.PropertiesPreferencePage" class="org.emftext.language.java_properties.resource.properties.ui.PropertiesPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.java_properties.resource.properties.ui.PropertiesSyntaxColoringPreferencePage" class="org.emftext.language.java_properties.resource.properties.ui.PropertiesSyntaxColoringPreferencePage" category="org.emftext.language.java_properties.resource.properties.ui.PropertiesPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.java_properties.resource.properties.ui.PropertiesBracketPreferencePage" class="org.emftext.language.java_properties.resource.properties.ui.PropertiesBracketPreferencePage" category="org.emftext.language.java_properties.resource.properties.ui.PropertiesPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.java_properties.resource.properties.ui.PropertiesOccurrencePreferencePage" class="org.emftext.language.java_properties.resource.properties.ui.PropertiesOccurrencePreferencePage" category="org.emftext.language.java_properties.resource.properties.ui.PropertiesPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java_properties.resource.properties.ui.PropertiesNewFileWizard" id="org.emftext.language.java_properties.resource.properties.ui.PropertiesNewFileWizard" name="EMFText .properties file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.java_properties.resource.properties.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.java_properties.resource.properties.nature" name="properties nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.java_properties.resource.properties.mopp.PropertiesNature" />
		</runtime>
		<builder id="org.emftext.language.java_properties.resource.properties.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.java_properties.resource.properties.builder" name="properties Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.java_properties.resource.properties.mopp.PropertiesBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.java_properties.resource.properties.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java_properties.resource.properties.mopp.PropertiesResourceFactoryDelegator" type="properties">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.java_properties.resource.properties.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
