<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java.treejava.resource.treejava.TreejavaResourceFactory" type="treejava">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.syntax">
		<metaInformationProvider class="org.emftext.language.java.treejava.resource.treejava.TreejavaMetaInformation" id="org.emftext.language.java.treejava.resource.treejava.TreejavaMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="treejava">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java.treejava.resource.treejava.TreejavaNewFileWizard" id="org.emftext.language.java.treejava.resource.treejava.TreejavaNewFileWizard" name="EMFText .treejava file">
		</wizard>
	</extension>
	
</plugin>
