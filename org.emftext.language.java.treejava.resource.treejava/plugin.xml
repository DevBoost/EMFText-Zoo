<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension point="org.emftext.language.java.treejava.resource.treejava.default_load_options">
    <provider
          class="org.emftext.language.java.treejava.resource.treejava.compiler.TreeJavaCompiler"
          id="org.emftext.language.java.treejava.resource.treejava.compiler.TreeJavaCompiler">
    </provider>
   </extension>
   
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java.treejava.resource.treejava.mopp.TreejavaResourceFactory" type="treejava">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.java.treejava.resource.treejava.mopp.TreejavaTextResourcePluginMetaInformation" id="org.emftext.language.java.treejava.resource.treejava.TreejavaMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="treejava">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaNewFileWizard" id="org.emftext.language.java.treejava.resource.treejava.TreejavaNewFileWizard" name="EMFText .treejava file">
		</wizard>
	</extension>

   
   <extension
         point="org.emftext.language.java.treejava.resource.treejava.default_load_options">
      <provider
            class="org.emftext.language.java.resource.java.analysis.helper.JavaPostProcessor"
            id="org.emftext.language.java.expression_simplifier_option_provider">
      </provider>
   </extension>

	<extension-point id="org.emftext.language.java.treejava.resource.treejava.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>

	
</plugin>
