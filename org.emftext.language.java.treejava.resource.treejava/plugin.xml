<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.java.treejava.resource.treejava.mopp.TreejavaMetaInformation" id="org.emftext.language.java.treejava.resource.treejava.mopp.TreejavaMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="treejava" icon="icons/editor_icon.gif" id="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaEditor" name="EMFText treejava Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Treejava Text Editor" id="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaPreferencePage" class="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaPreferencePage">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaSyntaxColoringPreferencePage" class="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaSyntaxColoringPreferencePage" category="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaBracketPreferencePage" class="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaBracketPreferencePage" category="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaOccurrencePreferencePage" class="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaOccurrencePreferencePage" category="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaNewFileWizard" id="org.emftext.language.java.treejava.resource.treejava.ui.TreejavaNewFileWizard" name="EMFText .treejava file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.java.treejava.resource.treejava.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.language.java.treejava.resource.treejava.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java.treejava.resource.treejava.mopp.TreejavaResourceFactoryDelegator" type="treejava">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.java.treejava.resource.treejava.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
