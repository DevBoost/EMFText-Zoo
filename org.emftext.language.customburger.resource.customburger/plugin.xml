<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.customburger.resource.customburger.CustomburgerResourceFactory" type="customburger">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.concretesyntax">
		<concretesyntax uri="http://www.emftext.org/language/customburger" csName="customburger" csDefinition="org.emftext.language.customburger/metamodel/customburger.cs">
		</concretesyntax>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="customburger">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.customburger.resource.customburger.CustomburgerNewFileWizard" id="org.emftext.language.customburger.resource.customburger.CustomburgerNewFileWizard" name="EMFText .customburger file">
		</wizard>
	</extension>
	
</plugin>
