<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.emftext.language.java.resource.JavaSourceOrClassFileResourceFactoryImpl"
            type="class">
      </parser>
   </extension>

   <extension
         point="org.emftext.language.java.resource.java.additional_extension_parser">
      <parser
            class="org.emftext.language.java.resource.JavaSourceOrClassFileResourceFactoryImpl">
      </parser>
   </extension>
   
   <extension
         point="org.emftext.language.java.resource.java.default_load_options">
      <provider
            class="org.emftext.language.java.resource.util.UnicodeConverterProvider"
            id="org.emftext.language.java.resource.util.UnicodeConverterProvider">
      </provider>
   </extension>
   
   <extension
         point="org.emftext.language.java.resource.java.default_load_options">
      <provider
            class="org.emftext.language.java.resource.util.JavaPostProcessor"
            id="org.emftext.language.java.resource.util.JavaPostProcessor">
      </provider>
   </extension>
 
   <extension
         point="org.eclipse.ui.popupMenus">    
      <objectContribution
            adaptable="true"
            id="org.emftext.language.java.resource.register.action"
            objectClass="org.eclipse.core.resources.IFolder">
         <action
               class="org.emftext.language.java.resource.register.RegisterJavaFilesAction"
               id="org.emftext.language.java.resource.register.action"
               label="Register In Classpath"
               menubarPath="org.emftext.language.java/actions">
         </action>
         <menu
               id="org.emftext.language.java"
               label="JaMoPP"
               path="additions">
            <separator
                  name="actions">
            </separator>
         </menu>
      </objectContribution>
  </extension>
     
</plugin>
