<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.filesystem.resource.fs.mopp.FsResourceFactory" type="fs">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.filesystem.resource.fs.mopp.FsMetaInformation" id="org.emftext.language.filesystem.resource.fs.mopp.FsMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="org.emftext.filetype.fs" name="EMFText fs Files">
		</content-type>
		<file-association content-type="org.emftext.filetype.fs" file-extensions="fs">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.filesystem.resource.fs.ui.FsEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" icon="icons/editor_icon.gif" id="org.emftext.language.filesystem.resource.fs.ui.FsEditor" name="EMFText fs Editor">
			<contentTypeBinding contentTypeId="org.eclipse.core.runtime.text">
			</contentTypeBinding>
			<contentTypeBinding contentTypeId="org.emftext.filetype.fs" >
			</contentTypeBinding>
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.filesystem.resource.fs.ui.FsPreferenceInitializer">
		</initializer>
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.filesystem.resource.fs.ui.FsNewFileWizard" id="org.emftext.language.filesystem.resource.fs.ui.FsNewFileWizard" name="EMFText .fs file">
		</wizard>
	</extension>
	
</plugin>
