<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.filesystem.resource.fs.mopp.FsMetaInformation" id="org.emftext.language.filesystem.resource.fs.mopp.FsMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.filesystem.resource.fs.ui.FsEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="fs" icon="icons/editor_icon.gif" id="org.emftext.language.filesystem.resource.fs.ui.FsEditor" name="EMFText fs Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.filesystem.resource.fs.ui.FsPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Fs Text Editor" id="org.emftext.language.filesystem.resource.fs.ui.FsPreferencePage" class="org.emftext.language.filesystem.resource.fs.ui.FsPreferencePage" category="org.eclipse.ui.preferencePages.GeneralTextEditor">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.filesystem.resource.fs.ui.FsSyntaxColoringPreferencePage" class="org.emftext.language.filesystem.resource.fs.ui.FsSyntaxColoringPreferencePage" category="org.emftext.language.filesystem.resource.fs.ui.FsPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.filesystem.resource.fs.ui.FsBracketPreferencePage" class="org.emftext.language.filesystem.resource.fs.ui.FsBracketPreferencePage" category="org.emftext.language.filesystem.resource.fs.ui.FsPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.filesystem.resource.fs.ui.FsOccurrencePreferencePage" class="org.emftext.language.filesystem.resource.fs.ui.FsOccurrencePreferencePage" category="org.emftext.language.filesystem.resource.fs.ui.FsPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.filesystem.resource.fs.ui.FsNewFileWizard" id="org.emftext.language.filesystem.resource.fs.ui.FsNewFileWizard" name="EMFText .fs file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.filesystem.resource.fs.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension id="org.emftext.language.filesystem.resource.fs.nature" name="fs nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.filesystem.resource.fs.mopp.FsNature" />
		</runtime>
		<builder id="org.emftext.language.filesystem.resource.fs.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.filesystem.resource.fs.builder" name="fs Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.filesystem.resource.fs.mopp.FsBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.filesystem.resource.fs.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.filesystem.resource.fs.mopp.FsResourceFactoryDelegator" type="fs">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.filesystem.resource.fs.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
