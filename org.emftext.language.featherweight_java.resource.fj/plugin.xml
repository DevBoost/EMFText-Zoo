<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.featherweight_java.resource.fj.mopp.FjResourceFactory" type="fj">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.featherweight_java.resource.fj.mopp.FjMetaInformation" id="org.emftext.language.featherweight_java.resource.fj.mopp.FjMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="org.emftext.filetype.fj" name="EMFText fj Files">
		</content-type>
		<file-association content-type="org.emftext.filetype.fj" file-extensions="fj">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.featherweight_java.resource.fj.ui.FjEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" icon="icons/editor_icon.gif" id="org.emftext.language.featherweight_java.resource.fj.ui.FjEditor" name="EMFText fj Editor">
			<contentTypeBinding contentTypeId="org.eclipse.core.runtime.text">
			</contentTypeBinding>
			<contentTypeBinding contentTypeId="org.emftext.filetype.fj" >
			</contentTypeBinding>
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.featherweight_java.resource.fj.ui.FjPreferenceInitializer">
		</initializer>
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.featherweight_java.resource.fj.ui.FjNewFileWizard" id="org.emftext.language.featherweight_java.resource.fj.ui.FjNewFileWizard" name="EMFText .fj file">
		</wizard>
	</extension>
	
</plugin>
