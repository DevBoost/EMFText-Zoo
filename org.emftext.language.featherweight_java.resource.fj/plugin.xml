<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.featherweight_java.resource.fj.mopp.FjResourceFactory" type="fj">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.featherweight_java.resource.fj.mopp.FjMetaInformation" id="org.emftext.language.featherweight_java.resource.fj.mopp.FjMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.featherweight_java.resource.fj.ui.FjEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="fj" icon="icons/editor_icon.gif" id="org.emftext.language.featherweight_java.resource.fj.ui.FjEditor" name="EMFText fj Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.featherweight_java.resource.fj.ui.FjPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.featherweight_java.resource.fj.ui.FjNewFileWizard" id="org.emftext.language.featherweight_java.resource.fj.ui.FjNewFileWizard" name="EMFText .fj file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.featherweight_java.resource.fj.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.language.featherweight_java.resource.fj.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
</plugin>
