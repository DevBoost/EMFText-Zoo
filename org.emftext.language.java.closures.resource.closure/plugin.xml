<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java.closures.resource.closure.ClosureResourceFactory" type="closure">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.syntax">
		<metaInformationProvider class="org.emftext.language.java.closures.resource.closure.ClosureMetaInformation" id="org.emftext.language.java.closures.resource.closure.ClosureMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="closure">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java.closures.resource.closure.ClosureNewFileWizard" id="org.emftext.language.java.closures.resource.closure.ClosureNewFileWizard" name="EMFText .closure file">
		</wizard>
	</extension>
	
	  <extension
         point="org.emftext.runtime.default_load_options">
      <provider
      		csName="closure"
            class="org.emftext.language.java.resource.java.analysis.helper.UnicodeConverterOptionProvider"
            id="org.emftext.language.java.unicode_converter_option_provider">
      </provider>
   </extension>
   
   <extension
         point="org.emftext.runtime.default_load_options">
      <provider
            class="org.emftext.language.java.resource.java.analysis.helper.JavaPostProcessor"
            csName="closure"
            id="org.emftext.language.java.expression_simplifier_option_provider">
      </provider>
   </extension>
   

	
	
	
</plugin>
