"Encapsulate In Panel" maps <ExtractXwithReferenceClass> {

	Extract := screenmodel.Widget;
	
	OrigContainer := screenmodel.Composite {
		extracts := compounds;
	};
	
	NewContainer := widgets.Panel(newName -> name){
		moved := compounds;
	};
	
	ContainerContainer := screenmodel.Composite {
		source := compounds;
		target := compounds;
	};
}