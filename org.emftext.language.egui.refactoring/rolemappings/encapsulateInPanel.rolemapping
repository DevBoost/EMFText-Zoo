ROLEMODELMAPPING FOR <http://www.emftext.org/language/egui> 

EncapsulateInPanel maps <ExtractXwithReferenceClass> {

	Extract := screenmodel.Widget;
	
	OrigContainer := screenmodel.Composite {
		extracts := children;
	};
	
	NewContainer := widgets.Panel(newName -> name){
		moved := children;
	};
	
	ContainerContainer := screenmodel.Composite {
		source := children;
		target := children;
	};
} 
