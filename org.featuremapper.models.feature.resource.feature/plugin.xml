<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.featuremapper.models.feature.resource.feature.FeatureResourceFactory"
            type="feature">
      </parser>
   </extension>

   <extension
         point="org.emftext.runtime.concretesyntax">
      <concretesyntax
            uri="http://www.tudresden.de/feature"
            csName="feature"
            csDefinition="org.emftext.language.feature/metamodel/feature.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.emftext.filetype"
            file-extensions="feature">
      </file-association>
   </extension>

</plugin>
