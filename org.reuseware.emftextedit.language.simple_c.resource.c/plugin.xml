<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.reuseware.emftextedit.language.simple_c.resource.c.CResourceFactoryImpl"
            type="c">
      </parser>
   </extension>

   <extension
         point="org.reuseware.emftextedit.concretesyntax">
      <concretesyntax
            uri="http://www.reuseware.org/simple_c"
            csName="c"
            csDefinition="org.reuseware.emftextedit.language.simple_c/metamodel/c.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.reuseware.emftextedit.filetype"
            file-extensions="c">
      </file-association>
   </extension>

</plugin>
