<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.java.sqljava.resource.sqljava.mopp.SqljavaMetaInformation" id="org.emftext.language.java.sqljava.resource.sqljava.mopp.SqljavaMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.java.sqljava.resource.sqljava.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.java.sqljava.resource.sqljava.nature" name="sqljava nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.java.sqljava.resource.sqljava.mopp.SqljavaNature" />
		</runtime>
		<builder id="org.emftext.language.java.sqljava.resource.sqljava.builder" />
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.java.sqljava.resource.sqljava.builder" name="sqljava Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.java.sqljava.resource.sqljava.mopp.SqljavaBuilderAdapter" />
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.java.sqljava.resource.sqljava.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java.sqljava.resource.sqljava.mopp.SqljavaResourceFactoryDelegator" type="sqljava">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.java.sqljava.resource.sqljava.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
