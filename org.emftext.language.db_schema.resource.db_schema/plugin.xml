<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.db_schema.resource.db_schema.mopp.Db_schemaResourceFactory" type="db_schema">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.db_schema.resource.db_schema.mopp.Db_schemaMetaInformation" id="org.emftext.language.db_schema.resource.db_schema.mopp.Db_schemaMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="db_schema">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.db_schema.resource.db_schema.ui.Db_schemaNewFileWizard" id="org.emftext.language.db_schema.resource.db_schema.ui.Db_schemaNewFileWizard" name="EMFText .db_schema file">
		</wizard>
	</extension>
	
</plugin>
