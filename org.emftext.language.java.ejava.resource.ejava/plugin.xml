<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.emftext.language.java.ejava.resource.ejava.default_load_options">
      <provider
            class="org.emftext.language.java.ejava.resource.ejava.EJavaPostProcessor"
            id="org.emftext.language.java.expression_simplifier_option_provider">
      </provider>
   </extension>
   
   <extension
         point="org.emftext.language.java.ejava.resource.ejava.default_load_options">
      <provider
            class="org.emftext.language.java.resource.java.analysis.helper.JavaPostProcessor"
            id="org.emftext.language.java.expression_simplifier_option_provider">
      </provider>
   </extension>

	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java.ejava.resource.ejava.ExtendedEjavaResourceFactory" type="ejava">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.java.ejava.resource.ejava.mopp.EjavaTextResourcePluginMetaInformation" id="org.emftext.language.java.ejava.resource.ejava.EjavaMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="ejava">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java.ejava.resource.ejava.ui.EjavaNewFileWizard" id="org.emftext.language.java.ejava.resource.ejava.EjavaNewFileWizard" name="EMFText .ejava file">
		</wizard>
	</extension>

	<extension-point id="org.emftext.language.java.ejava.resource.ejava.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>

	
</plugin>
