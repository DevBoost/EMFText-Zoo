<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.mopp.MartinfowlerdslMetaInformation" id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.mopp.MartinfowlerdslMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" extensions="martinfowlerdsl" icon="icons/editor_icon.gif" id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslEditor" name="EMFText martinfowlerdsl Editor">
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.preferencePages">
		<page name="Martinfowlerdsl Text Editor" id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslPreferencePage" class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslPreferencePage">
		</page>
		<page name="Syntax Coloring" id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslSyntaxColoringPreferencePage" class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslSyntaxColoringPreferencePage" category="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslPreferencePage">
		</page>
		<page name="Brackets" id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslBracketPreferencePage" class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslBracketPreferencePage" category="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslPreferencePage">
		</page>
		<page name="Occurrence" id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslOccurrencePreferencePage" class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslOccurrencePreferencePage" category="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslPreferencePage">
		</page>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslNewFileWizard" id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.ui.MartinfowlerdslNewFileWizard" name="EMFText .martinfowlerdsl file">
		</wizard>
	</extension>
	
	<extension id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
	</extension>
	
	<extension-point id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.mopp.MartinfowlerdslResourceFactoryDelegator" type="martinfowlerdsl">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.martinfowlerdsl.resource.martinfowlerdsl.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd"/>
	
</plugin>
