<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.km3.resource.km3.Km3ResourceFactory" type="km3">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.concretesyntax">
		<concretesyntax uri="http://www.eclipse.org/gmt/2005/KM3" csName="km3" csDefinition="org.emftext.language.km3/metamodel/KM3.cs">
		</concretesyntax>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="km3">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.km3.resource.km3.Km3NewFileWizard" id="org.emftext.language.km3.resource.km3.Km3NewFileWizard" name="EMFText .km3 file">
		</wizard>
	</extension>
	
</plugin>
