<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.emf.ecore.extension_parser">
      <parser
            class="org.emftext.language.java.reusejava.resource.reusejava.ReusejavaResourceFactoryImpl"
            type="reusejava">
      </parser>
   </extension>

   <extension
         point="org.emftext.runtime.concretesyntax">
      <concretesyntax
            uri="http://www.emftext.org/reusejava"
            csName="reusejava"
            csDefinition="org.emftext.language.java.reusejava/metamodel/reusejava.cs">
      </concretesyntax>
   </extension>

   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <file-association
            content-type="org.emftext.filetype"
            file-extensions="reusejava">
      </file-association>
   </extension>

</plugin>
