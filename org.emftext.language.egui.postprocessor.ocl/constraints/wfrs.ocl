package egui::statemodel

context State::isStartState
derive: StateModel.allInstances()->collect(startStates)->includes(self)

context State::isFinalState
derive: StateModel.allInstances()->collect(finalStates)->includes(self)

context StateModel
inv extactOneInitialState:
	states->select(isStartState)->size() = 1
inv atLeastOneEndState:
	states->select(isFinalState)->size() >= 1

endpackage