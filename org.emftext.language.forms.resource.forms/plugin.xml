<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.forms.resource.forms.mopp.FormsResourceFactory" type="forms">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.forms.resource.forms.mopp.FormsMetaInformation" id="org.emftext.language.forms.resource.forms.mopp.FormsMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="forms">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.forms.resource.forms.ui.FormsNewFileWizard" id="org.emftext.language.forms.resource.forms.ui.FormsNewFileWizard" name="EMFText .forms file">
		</wizard>
	</extension>
	
</plugin>
