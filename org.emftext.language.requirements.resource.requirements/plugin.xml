<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.requirements.resource.requirements.mopp.RequirementsMetaInformation" id="org.emftext.language.requirements.resource.requirements.mopp.RequirementsMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension id="org.emftext.language.requirements.resource.requirements.problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.eclipse.core.resources.problemmarker">
		</super>
		<super type="org.eclipse.core.resources.textmarker">
		</super>
		<super type="org.eclipse.emf.ecore.diagnostic">
		</super>
	</extension>
	
	<extension id="org.emftext.language.requirements.resource.requirements.problem.syntax_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.requirements.resource.requirements.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.requirements.resource.requirements.problem.print_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.requirements.resource.requirements.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.requirements.resource.requirements.problem.unresolved_reference" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.requirements.resource.requirements.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.requirements.resource.requirements.problem.analysis_problem" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.requirements.resource.requirements.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.requirements.resource.requirements.problem.builder_error" name="EMFText Problem" point="org.eclipse.core.resources.markers">
		<persistent value="true">
		</persistent>
		<super type="org.emftext.language.requirements.resource.requirements.problem">
		</super>
	</extension>
	
	<extension id="org.emftext.language.requirements.resource.requirements.nature" name="requirements nature" point="org.eclipse.core.resources.natures">
		<runtime>
			<run class="org.emftext.language.requirements.resource.requirements.mopp.RequirementsNature">
			</run>
		</runtime>
		<builder id="org.emftext.language.requirements.resource.requirements.builder">
		</builder>
	</extension>
	
	<extension point="org.eclipse.core.resources.builders" id="org.emftext.language.requirements.resource.requirements.builder" name="requirements Builder">
		<builder hasNature="true">
			<run class="org.emftext.language.requirements.resource.requirements.mopp.RequirementsBuilderAdapter">
			</run>
		</builder>
	</extension>
	
	<extension-point id="org.emftext.language.requirements.resource.requirements.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd">
	</extension-point>
	
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.requirements.resource.requirements.mopp.RequirementsResourceFactoryDelegator" type="requirements">
		</parser>
	</extension>
	
	<extension-point id="org.emftext.language.requirements.resource.requirements.additional_extension_parser" name="Additional Extension Parser" schema="schema/additional_extension_parser.exsd">
	</extension-point>
	
</plugin>
