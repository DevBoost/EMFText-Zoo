<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.c_sharp.resource.csharp.CsharpResourceFactory" type="csharp">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.concretesyntax">
		<concretesyntax uri="http://www.emftext.org/c_sharp" csName="csharp" csDefinition="org.emftext.language.c_sharp/metamodel/c_sharp.cs">
		</concretesyntax>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="csharp">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="\icons\default_new_icon.gif" class="org.emftext.language.c_sharp.resource.csharp.CsharpNewFileAction" id="org.emftext.language.c_sharp.resource.csharp.CsharpNewFileAction" name="EMFText .csharp file">
		</wizard>
	</extension>
	
</plugin>
