<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="c_sharp"
    nsURI="http://www.emftext.org/c_sharp" nsPrefix="c_sharp">
  <eSubpackages name="classes" nsURI="http://www.emftext.org/c_sharp/classes" nsPrefix="classes">
    <eClassifiers xsi:type="ecore:EClass" name="Class" eSuperTypes="#//namespaces/TypeDeclaration #//common/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="modifiers" upperBound="-1"
          eType="#//modifiers/Modifier" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="classBase" eType="#//classes/ClassBase"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="classMemberDeclarations"
          upperBound="-1" eType="#//classes/ClassMemberDeclaration" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ClassBase">
      <eStructuralFeatures xsi:type="ecore:EReference" name="types" lowerBound="1"
          upperBound="-1" eType="#//types/ClassOrInterfaceOrDelegateOrEnumType" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ClassMemberDeclaration" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="Method" eSuperTypes="#//classes/ClassMemberDeclaration #//common/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="modifiers" upperBound="-1"
          eType="#//modifiers/Modifier" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="returnType" eType="#//types/Type"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="interfaceType" eType="#//types/ClassOrInterfaceOrDelegateOrEnumType"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="block" lowerBound="1"
          eType="#//classes/Block" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Block">
      <eStructuralFeatures xsi:type="ecore:EReference" name="statement" upperBound="-1"
          eType="#//statements/Statement" containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="common" nsURI="http://www.emftext.org/c_sharp/common" nsPrefix="common">
    <eClassifiers xsi:type="ecore:EClass" name="NamedElement" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NamespaceOrTypeName">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parts" lowerBound="1"
          upperBound="-1" eType="#//common/NamespaceOrTypeNamePart" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NamespaceOrTypeNamePart" eSuperTypes="#//common/NamedElement"/>
  </eSubpackages>
  <eSubpackages name="expressions" nsURI="http://www.emftext.org/c_sharp/expressions"
      nsPrefix="expressions">
    <eClassifiers xsi:type="ecore:EClass" name="Expression" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="ConditionalExpression" eSuperTypes="#//expressions/Expression"/>
    <eClassifiers xsi:type="ecore:EClass" name="Assignment"/>
    <eClassifiers xsi:type="ecore:EClass" name="StatementExpression" abstract="true"/>
  </eSubpackages>
  <eSubpackages name="modifiers" nsURI="http://www.emftext.org/c_sharp/modifiers"
      nsPrefix="modifiers">
    <eClassifiers xsi:type="ecore:EClass" name="Modifier" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="Abstract" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Extern" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Internal" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="New" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="OverrideModifier" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Private" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Protected" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Public" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="ReadOnly" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Sealed" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Static" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Unsafe" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Virtual" eSuperTypes="#//modifiers/Modifier"/>
    <eClassifiers xsi:type="ecore:EClass" name="Volatile" eSuperTypes="#//modifiers/Modifier"/>
  </eSubpackages>
  <eSubpackages name="namespaces" nsURI="http://www.emftext.org/c_sharp/namespaces"
      nsPrefix="namespaces">
    <eClassifiers xsi:type="ecore:EClass" name="CompilationUnit">
      <eStructuralFeatures xsi:type="ecore:EReference" name="usingDirectives" upperBound="-1"
          eType="#//namespaces/UsingDirective" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="namespaceMemberDeclaration"
          upperBound="-1" eType="#//namespaces/NamespaceMemberDeclaration" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="UsingDirective" eSuperTypes="#//common/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="namespaceOrTypeName"
          lowerBound="1" eType="#//common/NamespaceOrTypeName" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NamespaceMemberDeclaration" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="Namespace" eSuperTypes="#//namespaces/NamespaceMemberDeclaration">
      <eStructuralFeatures xsi:type="ecore:EReference" name="namespaceName" lowerBound="1"
          eType="#//common/NamespaceOrTypeName" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="namespaceBody" lowerBound="1"
          eType="#//namespaces/NamespaceBody" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NamespaceBody">
      <eStructuralFeatures xsi:type="ecore:EReference" name="usingDirectives" upperBound="-1"
          eType="#//namespaces/UsingDirective" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="namespaceMemberDeclaration"
          upperBound="-1" eType="#//namespaces/NamespaceMemberDeclaration" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypeDeclaration" abstract="true" eSuperTypes="#//namespaces/NamespaceMemberDeclaration #//classes/ClassMemberDeclaration"/>
  </eSubpackages>
  <eSubpackages name="statements" nsURI="http://www.emftext.org/c_sharp/statements"
      nsPrefix="statements">
    <eClassifiers xsi:type="ecore:EClass" name="Statement" abstract="true" eSuperTypes="#//common/NamedElement"/>
    <eClassifiers xsi:type="ecore:EClass" name="LabeledStatement" eSuperTypes="#//statements/Statement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="statement" lowerBound="1"
          eType="#//statements/Statement" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DeclarationStatement" eSuperTypes="#//statements/Statement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="localVariableDeclaration"
          eType="#//statements/LocalVariableDeclaration" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="localConstantDeclaration"
          eType="#//statements/LocalConstantDeclaration" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="EmbeddedStatement" abstract="true"
        eSuperTypes="#//statements/Statement"/>
    <eClassifiers xsi:type="ecore:EClass" name="SimpleEmbeddedStatement" eSuperTypes="#//statements/EmbeddedStatement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="block" eType="#//classes/Block"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="EmptyStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="ExpressionStatement" eSuperTypes="#//statements/EmbeddedStatement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="statementExpression"
          eType="#//expressions/StatementExpression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SelectionStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="IterationStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="JumpStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="TryStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="CheckedStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="UncheckedStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="LockStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="UsingStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="FixedStatement" eSuperTypes="#//statements/EmbeddedStatement"/>
    <eClassifiers xsi:type="ecore:EClass" name="LocalVariableDeclaration">
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1"
          eType="#//types/Type" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="localVariableDeclarator"
          lowerBound="1" eType="#//statements/LocalVariableDeclarator" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LocalVariableDeclarator" eSuperTypes="#//common/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="localVariableInitializer"
          eType="#//statements/LocalVariableInitializer" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LocalVariableInitializer" abstract="true"
        eSuperTypes="#//expressions/Expression"/>
    <eClassifiers xsi:type="ecore:EClass" name="LocalConstantDeclaration">
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//types/Type"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="constantDeclarator" lowerBound="1"
          upperBound="-1" eType="#//statements/ConstantDeclarator" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ConstantDeclarator" eSuperTypes="#//common/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="expression" eType="#//expressions/Expression"
          containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="types" nsURI="http://www.emftext.org/c_sharp/types" nsPrefix="types">
    <eClassifiers xsi:type="ecore:EClass" name="Type" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="SimpleType" abstract="true" eSuperTypes="#//types/Type"/>
    <eClassifiers xsi:type="ecore:EClass" name="Void" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Decimal" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Bool" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="SByte" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Byte" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Short" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="UShort" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Int" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="UInt" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Long" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="ULong" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Char" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Float" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="Double" eSuperTypes="#//types/SimpleType"/>
    <eClassifiers xsi:type="ecore:EClass" name="ReferenceType" abstract="true" eSuperTypes="#//types/Type"/>
    <eClassifiers xsi:type="ecore:EClass" name="ClassOrInterfaceOrDelegateOrEnumType"
        eSuperTypes="#//types/ReferenceType">
      <eStructuralFeatures xsi:type="ecore:EReference" name="namespaceOrTypeName"
          lowerBound="1" eType="#//common/NamespaceOrTypeName" containment="true"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
