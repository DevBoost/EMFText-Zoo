PREFIX ex: <http//exampleorg/>
PREFIX userdef: <java:propertyfunction.>
CONSTRUCT { ?buch  ex:publishedBy ?verlag .
            ?buch  ex:title ?titelupp . 
            ?verlag  ex:name  ?name }
WHERE { ?buch ex:VerlegtBei ?verlag .
        ?buch ex:Titel ?titel .
        ?titel userdef:UpperCase ?titelupp.
        ?verlag ex:Name ?name }