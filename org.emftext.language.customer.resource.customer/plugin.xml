<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.customer.resource.customer.mopp.CustomerResourceFactory" type="customer">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.customer.resource.customer.mopp.CustomerTextResourcePluginMetaInformation" id="org.emftext.language.customer.resource.customer.mopp.CustomerTextResourcePluginMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<content-type id="org.emftext.filetype.customer" name="EMFText customer Files">
		</content-type>
		<file-association content-type="org.emftext.filetype.customer" file-extensions="customer">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.editors">
		<editor class="org.emftext.language.customer.resource.customer.ui.CustomerEditor" contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor" icon="icons/editor_icon.gif" id="org.emftext.language.customer.resource.customer.ui.CustomerEditor" name="EMFText customer Editor">
			<contentTypeBinding contentTypeId="org.eclipse.core.runtime.text">
			</contentTypeBinding>
			<contentTypeBinding contentTypeId="org.emftext.filetype.customer" >
			</contentTypeBinding>
		</editor>
	</extension>
	
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.emftext.language.customer.resource.customer.ui.CustomerPreferenceInitializer">
		</initializer>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.customer.resource.customer.ui.CustomerNewFileWizard" id="org.emftext.language.customer.resource.customer.ui.CustomerNewFileWizard" name="EMFText .customer file">
		</wizard>
	</extension>
	
	<extension-point id="org.emftext.language.customer.resource.customer.default_load_options" name="Default Load Options" schema="schema/default_load_options.exsd"/>
	
</plugin>
