<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.customer.resource.customer.mopp.CustomerResourceFactory" type="customer">
		</parser>
	</extension>
	
	<extension point="org.emftext.access.syntax">
		<metaInformationProvider class="org.emftext.language.customer.resource.customer.mopp.CustomerMetaInformation" id="org.emftext.language.customer.resource.customer.mopp.CustomerMetaInformation">
		</metaInformationProvider>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="customer">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.customer.resource.customer.ui.CustomerNewFileWizard" id="org.emftext.language.customer.resource.customer.ui.CustomerNewFileWizard" name="EMFText .customer file">
		</wizard>
	</extension>
	
</plugin>
