<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser class="org.emftext.language.java.javabehavior4uml.resource.javabehavior.JavabehaviorResourceFactory" type="javabehavior">
		</parser>
	</extension>
	
	<extension point="org.emftext.runtime.concretesyntax">
		<concretesyntax uri="http://www.emftext.org/javaBehavior4UML" csName="javabehavior" csDefinition="org.emftext.language.java.javabehavior4uml/metamodel/JavaBehavior4UML.cs">
		</concretesyntax>
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
		<file-association content-type="org.emftext.filetype" file-extensions="javabehavior">
		</file-association>
	</extension>
	
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.emftext.runtime.ui.EMFTextFileCategory" name="EMFText File">
		</category>
		<wizard category="org.emftext.runtime.ui.EMFTextFileCategory" icon="/icons/default_new_icon.gif" class="org.emftext.language.java.javabehavior4uml.resource.javabehavior.JavabehaviorNewFileWizard" id="org.emftext.language.java.javabehavior4uml.resource.javabehavior.JavabehaviorNewFileWizard" name="EMFText .javabehavior file">
		</wizard>
	</extension>
	
</plugin>
